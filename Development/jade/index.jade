doctype html
html(ng-app="PressKit")
	head
		meta(http-equiv="Content-Type" content="text/html; charset=utf-8")
		meta(name="viewport" content="initial-scale=1, maximum-scale=1, user-scalable=no")

		title Black Sails Season 3 Press Kit

		link(href="css/main.css" rel="stylesheet" type="text/css")
	body(ng-controller="PressKitController" ng-cloak ng-init="page='#{pages[6].path}';pagebg='#{pages[6].background}'; " style="background:{{pagebg}}")
		div.container
			include partials/header.jade

			div.content
				include partials/keyart.jade
				include partials/cast.jade
				include partials/production.jade
				include partials/gallery.jade
				include partials/videos.jade
				include partials/downloads.jade

			include partials/footer.jade

		script(type="text/javascript" src="js/plugins.min.js")
		script(type="text/javascript" src="js/app.min.js")


			



		
	